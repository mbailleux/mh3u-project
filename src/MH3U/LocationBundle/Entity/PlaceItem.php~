<?php

namespace MH3U\LocationBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Object
 *
 * @ORM\Table(name="mh3u_place_item")
 * @ORM\Entity(repositoryClass="MH3U\ItemBundle\Entity\Repository\ItemRepository")
 */
class PlaceItem
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     */
    private $id;

    /**
     * @ORM\ManyToOne(targetEntity="MH3U\ItemBundle\Entity\Item", cascade={"persist"})
     */
    private $item;

    /**
     * @ORM\ManyToOne(targetEntity="MH3U\LocationBundle\Entity\Place", cascade={"persist"})
     */
    private $place;

    /**
     * @ORM\ManyToOne(targetEntity="MH3U\LocationBundle\Entity\PlaceArea", cascade={"persist"})
     */
    private $area;

    /**
     * @ORM\ManyToOne(targetEntity="MH3U\LocationBundle\Entity\PlaceRank", cascade={"persist"})
     */
    private $rank;

    /**
     * @ORM\ManyToOne(targetEntity="MH3U\LocationBundle\Entity\PlaceItemType", cascade={"persist"})
     */
    private $type;

    /**
     * Set id
     *
     * @return Object
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

}
